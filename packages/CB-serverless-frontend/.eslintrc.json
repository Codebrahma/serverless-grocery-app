{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": [
      "error", {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ]
}